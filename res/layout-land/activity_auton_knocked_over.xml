<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sumeet.test.autonKnockedOver">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Totes knocked over"
        android:id="@+id/textView15"
        android:textSize="40dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton29"
        android:background="@drawable/customplus"
        android:onClick="addBin"
        android:layout_alignTop="@+id/imageButton31"
        android:layout_alignLeft="@+id/imageButton33"
        android:layout_alignStart="@+id/imageButton33" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton30"
        android:background="@drawable/customminus"
        android:onClick="subtractBin"
        android:layout_below="@+id/imageButton29"
        android:layout_alignLeft="@+id/imageButton29"
        android:layout_alignStart="@+id/imageButton29" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bins knocked over"
        android:id="@+id/textView16"
        android:textSize="40dp"
        android:layout_above="@+id/imageButton30"
        android:layout_toLeftOf="@+id/imageButton29"
        android:layout_toStartOf="@+id/imageButton29" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton31"
        android:background="@drawable/customplus"
        android:onClick="addTote"
        android:layout_alignBottom="@+id/textView15"
        android:layout_toRightOf="@+id/textView15"
        android:layout_toEndOf="@+id/textView15" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton32"
        android:background="@drawable/customminus"
        android:onClick="subtractTote"
        android:layout_below="@+id/imageButton31"
        android:layout_alignLeft="@+id/imageButton31" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton33"
        android:background="@drawable/checkmark"
        android:onClick="startTeleop"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
