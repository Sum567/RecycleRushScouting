<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sumeet.test.autonKnockedOver">

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton29"
        android:background="@drawable/customplus"
        android:onClick="addTote"
        android:layout_above="@+id/textView16"
        android:layout_toRightOf="@+id/textView16"
        android:layout_toEndOf="@+id/textView16" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton30"
        android:background="@drawable/customminus"
        android:onClick="subtractTote"
        android:layout_below="@+id/textView16"
        android:layout_alignRight="@+id/imageButton29"
        android:layout_alignEnd="@+id/imageButton29" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:hint="Totes knocked"
        android:editable="true"
        android:textSize="50dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton31"
        android:background="@drawable/checkmark"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="startTeleop" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:hint="Bins knocked"
        android:editable="true"
        android:textSize="50dp"
        android:layout_alignTop="@+id/textView16"
        android:layout_toLeftOf="@+id/imageButton31"
        android:layout_toStartOf="@+id/imageButton31" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton32"
        android:background="@drawable/customplus"
        android:onClick="addBin"
        android:layout_above="@+id/textView17"
        android:layout_alignRight="@+id/imageButton31"
        android:layout_alignEnd="@+id/imageButton31" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton19"
        android:background="@drawable/customminus"
        android:onClick="subtractBin"
        android:layout_alignStart="@+id/imageButton32"
        android:layout_alignTop="@+id/imageButton30" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView15"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/imageButton29"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="50dp" />
</RelativeLayout>
