<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sumeet.test.teleopKnockedOver">


    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton33"
        android:background="@drawable/customplus"
        android:onClick="addTote"
        android:layout_above="@+id/textView18"
        android:layout_toRightOf="@+id/textView18"
        android:layout_toEndOf="@+id/textView18" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton34"
        android:background="@drawable/customminus"
        android:onClick="subtractTote"
        android:layout_alignTop="@+id/imageButton37"
        android:layout_toRightOf="@+id/textView18"
        android:layout_toEndOf="@+id/textView18" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView18"
        android:hint="Totes knocked"
        android:editable="true"
        android:textSize="50dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton35"
        android:background="@drawable/checkmark"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="startComments" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:hint="Bins knocked"
        android:editable="true"
        android:textSize="50dp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/imageButton36"
        android:layout_toStartOf="@+id/imageButton36" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton36"
        android:background="@drawable/customplus"
        android:onClick="addBin"
        android:layout_above="@+id/textView19"
        android:layout_alignLeft="@+id/imageButton35"
        android:layout_alignStart="@+id/imageButton35" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton37"
        android:background="@drawable/customminus"
        android:onClick="subtractBin"
        android:layout_below="@+id/textView19"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />



</RelativeLayout>
