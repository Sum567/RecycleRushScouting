<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sumeet.test.Autonomous">

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton14"
        android:background="@drawable/customplus"
        android:onClick="addOne"
        android:layout_above="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton15"
        android:background="@drawable/customminus"
        android:onClick="subtractOne"
        android:layout_alignStart="@+id/imageButton14"
        android:layout_alignTop="@+id/imageButton19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:hint="Totes Set"
        android:editable="true"
        android:textSize="50dp"
        android:layout_above="@+id/imageButton15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="0" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton23"
        android:background="@drawable/checkmark"
        android:onClick="submitTotes"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="Robot move?"
        android:textSize="50dp"
        android:id="@+id/checkBox"
        android:onClick="setMoveTrue"
        android:layout_above="@+id/imageButton23"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:hint="0"
        android:editable="true"
        android:textSize="50dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton17"
        android:background="@drawable/customplus"
        android:onClick="add"
        android:layout_above="@+id/textView10"
        android:layout_toLeftOf="@+id/imageButton23" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton19"
        android:background="@drawable/customminus"
        android:onClick="subtract"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/imageButton17"
        android:layout_alignStart="@+id/imageButton17" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Totes Set"
        android:textSize="40dp"
        android:id="@+id/textView20"
        android:layout_above="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_alignTop="@+id/imageButton14" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Bins Set"
        android:textSize="40dp"
        android:id="@+id/textView21"
        android:layout_above="@+id/textView10"
        android:layout_toRightOf="@+id/checkBox"
        android:layout_marginLeft="93dp"
        android:layout_marginStart="93dp"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_toLeftOf="@+id/imageButton17"
        android:layout_toStartOf="@+id/imageButton17" />

</RelativeLayout>
