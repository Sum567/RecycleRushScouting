<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sumeet.test.Autonomous">

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton14"
        android:background="@drawable/customplus"
        android:onClick="addToteSet"
        android:layout_alignTop="@+id/textView20"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton15"
        android:background="@drawable/customminus"
        android:onClick="subtractToteSet"
        android:layout_below="@+id/textView20"
        android:layout_toRightOf="@+id/textView20"
        android:layout_toEndOf="@+id/textView20" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:hint="Totes Set"
        android:editable="true"
        android:textSize="50dp"
        android:layout_above="@+id/imageButton15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="0" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton23"
        android:background="@drawable/checkmark"
        android:onClick="submitTotes"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="Robot move?"
        android:textSize="50dp"
        android:id="@+id/checkBox"
        android:onClick="setMoveTrue"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:hint="0"
        android:editable="true"
        android:textSize="50dp"
        android:layout_below="@+id/imageButton14"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton17"
        android:background="@drawable/customplus"
        android:onClick="addBinSet"
        android:layout_marginRight="104dp"
        android:layout_marginEnd="104dp"
        android:layout_above="@+id/textView10"
        android:layout_toLeftOf="@+id/imageButton23"
        android:layout_toStartOf="@+id/imageButton23" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton19"
        android:background="@drawable/customminus"
        android:onClick="subtractBinSet"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/imageButton17"
        android:layout_alignStart="@+id/imageButton17" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Totes Set"
        android:textSize="40dp"
        android:id="@+id/textView20"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/imageButton14"
        android:layout_toStartOf="@+id/imageButton14" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Bins Set"
        android:textSize="40dp"
        android:id="@+id/textView21"
        android:layout_above="@+id/textView10"
        android:layout_toRightOf="@+id/checkBox"
        android:layout_marginLeft="93dp"
        android:layout_marginStart="93dp"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_toStartOf="@+id/imageButton17" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Totes Stacked"
        android:textSize="40dp"
        android:id="@+id/textView22"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageButton15"
        android:layout_alignStart="@+id/imageButton15"
        android:layout_toLeftOf="@+id/textView10"
        android:layout_toStartOf="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:hint="0"
        android:editable="true"
        android:textSize="50dp"
        android:layout_below="@+id/textView22"
        android:layout_alignLeft="@+id/textView22"
        android:layout_alignStart="@+id/textView22" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton38"
        android:background="@drawable/customplus"
        android:onClick="addToteStacked"
        android:layout_alignTop="@+id/textView22"
        android:layout_toRightOf="@+id/textView10"
        android:layout_toEndOf="@+id/textView10" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton39"
        android:background="@drawable/customminus"
        android:onClick="subtractToteStacked"
        android:layout_below="@+id/textView22"
        android:layout_alignLeft="@+id/imageButton38"
        android:layout_alignStart="@+id/imageButton38" />

</RelativeLayout>
